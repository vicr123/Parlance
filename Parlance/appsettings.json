{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Vicr123Accounts": {
    "DbusConnectionPath": "unix:path=/var/vicr123-accounts/vicr123-accounts-bus"
  },
  "Parlance": {
    "RepositoryDirectory": "$HOME/Parlance/vcs",
    "DatabaseConnectionString": "Host=localhost;Username=parlance;Password=secret;Database=parlancedb",
    "UseDummyAuthenticationService": false,
    "ServerName": "Example Server",
    "AccountName": "Parlance"
  },
  "fido2": {
    "Origins": [
      "https://parlance.local:5173",
      "https://localhost:5173"
    ],
    "ServerDomain": "parlance.local"
  },
  "Email": {
    "RootUrl": "https://localhost:5173",
    "FromAddress": "parlance@parlance.com",
    "FromName": "Parlance",
    "UnsubscribeTokenSecret": "ChangeMe!!!"
  },
  "rateLimiting": {
    "PermitLimit": 8000,
    "Window": 3600,
    "SegmentsPerWindow": 4
  },
  "ForwardedHeaders": {
    "ForwardLimit": 0
  }
}
